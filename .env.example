# Required
ANTHROPIC_API_KEY=sk-ant-...

# Neo4j (used by store node)
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=memory-system

# Logging (path to JSON dictConfig file; default: logging.json)
# LOG_CONFIG=logging.debug-agents.json

# LLM
LLM_MODEL=claude-sonnet-4-20250514

# P2P (used by distributed mode)
POLL_INTERVAL=30

# Schema (path to persistent schema file; default: data/schema.yaml)
# SCHEMA_PATH=data/schema.yaml
